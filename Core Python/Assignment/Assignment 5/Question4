num = int(input("Enter a number: "))

# Convert number to string to get number of digits
digits = str(num)
power = len(digits)

sum_of_powers = 0

for d in digits:
    sum_of_powers += int(d) ** power

if sum_of_powers == num:
    print(num, "is an Armstrong number")
else:
    print(num, "is not an Armstrong number")
